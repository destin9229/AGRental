@model List<AGRental.Models.User>

@{
    ViewBag.Title = "Users Index";
}
<h1>@ViewBag.Title</h1>

@if (ViewBag.Username.Length > 0)
{
    <h3>Welcome, @ViewBag.Username !!!</h3>
    <p class="text-success">You have Succsessfully Logged In @ViewBag.Username <span class="glyphicon glyphicon-ok " aria-hidden="true"></span></p>
}


<p><a asp-controller="User" asp-action="SignUp">User SignUp</a> | <a asp-controller="User" asp-action="Login">Login</a> | <a asp-controller="User" asp-action="Logout">Logout</a></p>
